<% content_for :head do %>
	<title>BFF::Edit Fingering</title>
	<%= javascript_include_tag 'jquery', 'fingerings' %>
	<script>var fingering = @fingering.fingering_status;</script>
<% end %>

<% content_for :header do %>
	<a href="index.html"><img id="" src="" alt="" /></a>
	<%= render "layouts/login" %>
<% end %>

<%= content_for :headline do %>
	<div id="headline">
		<span class="main">Edit Fingering ID #<%= @fingering.id %></span>
	</div>
<% end %>

<% content_for :content do %>
	<p>This is still a work in progress!</p>
	<canvas id="edit_fingering" width="500" height="500">
    	<p>Unfortunately, your browser is currently unsupported by our web 
        	application. We are sorry for the inconvenience. Please use one of the 
        	supported browsers listed below. </p>
        <p>Supported browsers: 
        	<a href="www.mozilla.com">Firefox</a> <br />
        	<a href="www.google.com/chrome">Chrome</a> </p>
	</canvas>

	<%= link_to 'Show', @fingering %> |
	<%= link_to 'Back', fingerings_path %>
<% end %>



