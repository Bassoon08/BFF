<% content_for :head do %>
	<title>BFF::Fingering</title>
	<%= javascript_include_tag :fingerings %>
<% end %>

<% content_for :header do %>
	<a href="index.html"><img id="" src="" alt="" /></a>
	<%= render "layouts/login" %>
<% end %>

<%= content_for :headline do %>
	<div id="headline">
		<span class="main">Fingering ID #<%= @fingering.id %></span>
	</div>
<% end %>

<% content_for :content do %>
	<p id="notice"><%= notice %></p>
	<%= link_to 'Edit', edit_fingering_path(@fingering) %> |
	<%= link_to 'Back', fingerings_path %>
<% end %>
